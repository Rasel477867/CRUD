@model Student
<form method="post">
    <div class="border p-4 mt-3">
        <div class="row pb-2">
            <h2 class="text-primary">
                Create Student
            </h2>
            <hr />
        </div>
        <div class="mb-3">
            <label asp-for="Name">

            </label>
            <input asp-for="Name" class="form-control" placeholder="student name" />
        </div>
        <div class="mb-3">
            <label asp-for="Age">

            </label>
            <input asp-for="Age" class="form-control" placeholder="department name" />
        </div>
        <button type="submit" class="btn btn-primary">submit</button>
        <a asp-controller="Student" asp-action="Index" class="btn btn-dark">
            back studdent list
        </a>
    </div>
</form>